{
  "compounds": [
    {
      "configurations": ["Astro", "Payload"],
      "name": "Astro + Payload",
      "presentation": {
        "group": "Astro + Payload"
      },
      "stopAll": true
    },
    {
      "configurations": ["Next", "Payload"],
      "name": "Next + Payload",
      "presentation": {
        "group": "Next + Payload"
      },
      "stopAll": true
    },
    {
      "configurations": ["Svelte", "Payload"],
      "name": "Svelte + Payload",
      "presentation": {
        "group": "Svelte + Payload"
      },
      "stopAll": true
    }
  ],
  "configurations": [
    {
      "command": "pnpm dev --filter astro...",
      "cwd": "${workspaceFolder}/",
      "name": "Astro",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node-terminal"
    },
    {
      "command": "pnpm dev --filter next...",
      "cwd": "${workspaceFolder}/",
      "name": "Next",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node-terminal"
    },
    {
      "command": "pnpm dev --filter payload...",
      "cwd": "${workspaceFolder}/",
      "name": "Payload",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node-terminal"
    },
    {
      "command": "pnpm dev --filter svelte...",
      "cwd": "${workspaceFolder}/",
      "name": "Svelte",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node-terminal"
    },
    {
      "command": "pnpm storybook --filter docs...",
      "cwd": "${workspaceFolder}/",
      "name": "Docs",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node-terminal"
    }
  ],
  "version": "0.2.0"
}
