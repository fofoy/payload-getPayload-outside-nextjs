{
  "name": "svelte",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "git clean -xdf .turbo node_modules",
    "dev": "dotenvx run --convention=nextjs -- cross-env NODE_OPTIONS='--no-deprecation --inspect' vite dev",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@local/prettier-config",
  "dependencies": {
    "@local/env": "workspace:*",
    "@local/payload": "workspace:*",
    "payload": "3.0.0-beta.126"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.22.0",
    "@local/eslint-config": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/tailwind-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@local/types": "workspace:*",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.9",
    "@sveltejs/kit": "^2.8.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "svelte": "^5.1.15",
    "svelte-check": "^4.0.7",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  }
}
