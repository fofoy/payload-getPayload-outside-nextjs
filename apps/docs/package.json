  },
  "devDependencies": {
    "@local/eslint-config": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/storybook-config": "workspace:*",
    "@local/tailwind-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "aws-cdk-lib": "2.132.1",
    "constructs": "10.3.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "react": "catalog:next15",
    "react-dom": "catalog:next15",
    "sst": "^2.43.7",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2"
