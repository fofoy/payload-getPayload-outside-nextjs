{
  "name": "@local/storybook-config",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./style.css": "./src/styles/index.css"
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@local/prettier-config",
  "eslintConfig": {
    "extends": [
      "@local/eslint-config/react",
      "@local/eslint-config/base"
    ],
    "root": true
  },
  "dependencies": {
    "@etchteam/storybook-addon-css-variables-theme": "^3.0.0",
    "@storybook/addon-a11y": "^8.3.0",
    "@storybook/addon-actions": "^8.3.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/addon-onboarding": "^8.3.0",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-viewport": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/nextjs": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@storybook/types": "^8.3.0",
    "css-loader": "^6.11.0",
    "postcss-loader": "^8.1.1",
    "storybook": "^8.3.0",
    "style-loader": "^3.3.4",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.93.0"
  },
  "devDependencies": {
    "@local/eslint-config": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@types/react": "catalog:next15",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "react": "catalog:next15",
    "react-dom": "catalog:next15",
    "typescript": "^5.6.2"
  }
}
